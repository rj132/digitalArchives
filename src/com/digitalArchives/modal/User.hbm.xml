<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="com.digitalArchives.modal.User" table="t_user">
        <id name="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name"></property>
        <property name="username" column="username"></property>
        <property name="password" column="password"></property>
        <property name="phone" column="phone"></property>
        <property name="age" column="age"></property>
        <property name="sex" column="sex"></property>
        <property name="id_card_type" column="id_card_type"></property>
        <property name="id_card_code" column="id_card_code"></property>
        <property name="email" column="email"></property>
        <property name="money" column="money"></property>
        <property name="create_time" column="create_time"></property>
        <property name="login_time" column="login_time"></property>
    </class>
</hibernate-mapping>